{
    "extends": "airbnb",
    "settings": {
      "import/core-modules": ["react", "prop-types", "config", "react-helmet"]
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "jsx-a11y/anchor-is-valid": [ "error", {
        "components": [ "Link" ],
        "specialLink": [ "to" ]
      }]
    },
    "globals": {
        "graphql": true
    }
}
